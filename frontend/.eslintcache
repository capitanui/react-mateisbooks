etails>\n          <Typography>\n            <ListGroup variant=\"flush\">\n              <ListGroup.Item className=\"border-0\">\n                <Form.Check\n                  type=\"switch\"\n                  id=\"instoc\"\n                  label=\"În Stoc\"\n                  checked={inStock}\n                  onChange={() =>\n                    dispatch(updateProductFilters({ inStock: \"toggle\" }))\n                  }\n                />\n              </ListGroup.Item>\n              <ListGroup.Item className=\"border-0\">\n                <Form.Check\n                  type=\"switch\"\n                  id=\"comingsoon\"\n                  label=\"În curând\"\n                  onChange={() =>\n                    dispatch(updateProductFilters({ comingSoon: \"toggle\" }))\n                  }\n                  checked={comingSoon}\n                />\n              </ListGroup.Item>\n            </ListGroup>\n\n            <div class=\"d-flex\">\n              <FormControl className={classes.formControl}>\n                <InputLabel id=\"demo-mutiple-checkbox-label\">\n                  Categorie\n                </InputLabel>\n                <Select\n                  labelId=\"demo-mutiple-checkbox-label\"\n                  id=\"demo-mutiple-checkbox\"\n                  multiple\n                  value={typeOfBookFilter}\n                  onChange={updateFilterTypeOfBooks}\n                  input={<Input />}\n                  renderValue={(selected) =>\n                    selected.length + \" Categorii selectate\"\n                  }\n                  MenuProps={MenuProps}\n                >\n                  {typeOfBookCategories.map((name) => (\n                    <MenuItem key={name} value={name}>\n                      <Checkbox checked={typeOfBookFilter.indexOf(name) > -1} />\n                      <ListItemText primary={name} />\n                    </MenuItem>\n                  ))}\n                </Select>\n              </FormControl>\n            </div>\n          </Typography>\n        </AccordionDetails>\n      </Accordion>\n    </div>\n  );\n}\n","/Users/ionutcapitanu/Javascript/cartileluimatei/frontend/src/screens/PovesteaNoastra.js",[],"/Users/ionutcapitanu/Javascript/cartileluimatei/frontend/src/components/RouteChangeTracker.js",[],{"ruleId":"189","replacedBy":"190"},{"ruleId":"191","replacedBy":"192"},{"ruleId":"193","severity":1,"message":"194","line":8,"column":8,"nodeType":"195","messageId":"196","endLine":8,"endColumn":31},{"ruleId":"189","replacedBy":"197"},{"ruleId":"191","replacedBy":"198"},{"ruleId":"193","severity":1,"message":"199","line":8,"column":27,"nodeType":"195","messageId":"196","endLine":8,"endColumn":30},{"ruleId":"193","severity":1,"message":"200","line":8,"column":32,"nodeType":"195","messageId":"196","endLine":8,"endColumn":35},"no-native-reassign",["201"],"no-negated-in-lhs",["202"],"no-unused-vars","'FacebookSendToMessenger' is defined but never used.","Identifier","unusedVar",["201"],["202"],"'Col' is defined but never used.","'Row' is defined but never used.","no-global-assign","no-unsafe-negation"]