  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.10",
    "path": "^0.12.7",
    "react-messenger-send-to-messenger": "^0.2.0",
    "react-responsive": "^8.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
